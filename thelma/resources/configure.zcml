    >
        <representer content_type="everest.mime.XmlMime">
            <option
                name="xml_schema"
                value="thelma:schemata/ChemicalStructure.xsd" />
            <option
                name="xml_ns"
                value="http://schemata.cenix-bioscience.com/chemicalstructure" />
            <option
