                name="xml_schema"
                value="thelma:schemata/ChemicalStructureType.xsd" />
            <option
                name="xml_ns"
                value="http://schemata.cenix-bioscience.com/chemicalstructuretype" />
            <option
                name="xml_prefix"
                value="chst" />
            <attribute name="molecule_types">
                <option
                    name="ignore"
                    value="false" />
                <option
                    name="write_as_link"
                    value="false" />
            </attribute>
            <attribute name="molecule_types.modifications">
                <option
                    name="ignore"
                    value="true" />
            </attribute>
            <attribute name="molecule_types.name">
                <option
                    name="ignore"
