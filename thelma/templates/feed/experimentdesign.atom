<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      metal:use-macro="master.macros['standard_feed']">
  <content_type xmlns="http://schemata.cenix-bioscience.com/content_type"
               name="application/vnd.everest+xml;type=experimentDesignCollection"
               metal:fill-slot="content_type" />
  <content type="application/vnd.everest+xml;type=experimentDesignMember"
           metal:fill-slot="content">
    <experimentdesign xmlns="http://schemata.cenix-bioscience.com/experiment-design">
      <label tal:content="member.label">Design XYZ</label>
      <description tal:content="member.description">Long description</description>
      <link xmlns="http://www.w3.org/2005/Atom"
            rel="http://relations.cenix-bioscience.com/rack-shape"
            type="application/atom+xml;type=entry"
            title="Nunc" href="/rack-shapes/nunc"
            tal:attributes="title member.rack_shape.label;
                            href resource_to_url(member.rack_shape, request)" />
    </experimentdesign>
  </content>
</feed>
