<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      metal:use-macro="master.macros['standard_feed']">
  <content_type xmlns="http://schemata.cenix-bioscience.com/content_type"
               name="application/vnd.everest+xml;type=tubeSpecsCollection"
               metal:fill-slot="content_type" />
  <content type="application/vnd.everest+xml;type=tubeSpecsMember"
           metal:fill-slot="content">
    <tubeSpecs xmlns="http://schemata.cenix-bioscience.com/container-specs">
      <label tal:content="member.label">Abgene tube (1.4 ml)</label>
      <description tal:content="member.description">Descriptive text about the tube</description>
      <maxVolume unit="l" tal:content="member.max_volume">0.0012</maxVolume>
      <deadVolume unit="l" tal:content="member.dead_volume">5e-06</deadVolume>
      <link xmlns="http://www.w3.org/2005/Atom"
            tal:omit-tag="member.manufacturer is None"
            rel="http://relations.cenix-bioscience.com/organization"
            type="application/atom+xml;type=entry"
            title="AbGene" href="/organizations/abgene"
            tal:attributes="title member.manufacturer.name;
                            href resource_to_url(member.manufacturer, request)" />
    </tubeSpecs>
  </content>
</feed>
