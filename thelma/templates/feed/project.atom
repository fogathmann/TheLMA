<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      metal:use-macro="master.macros['standard_feed']">
  <content_type xmlns="http://schemata.cenix-bioscience.com/content_type"
               name="application/vnd.everest+xml;type=projectCollection"
               metal:fill-slot="content_type" />

  <content type="application/vnd.everest+xml;type=projectMember"
           metal:fill-slot="content">
    <project xmlns="http://schemata.cenix-bioscience.com/project" >
      <label tal:content="member.label">Bayer LDLR</label>
      <creationDate tal:content="rfc3339(member.creation_date)">2010-11-08T11:16:37+01:00</creationDate>
      <leader>
          <link xmlns="http://schemata.cenix-bioscience.com/Shared"
                  rel="http://relations.cenix-bioscience.com/user"
                type="application/atom+xml;type=entry"
                title="James" href="/user/james"
                tal:omit-tag="member.leader is None"
                tal:attributes="title member.leader.username;
                href resource_to_url(member.leader, request)" />
      </leader>
      <customer>
          <link  xmlns="http://schemata.cenix-bioscience.com/Shared"
                 rel="http://relations.cenix-bioscience.com/organization"
                 type="application/atom+xml;type=entry"
                 title="ABI" href="/organizations/abi"
                 tal:omit-tag="member.customer is None"
                 tal:attributes="title member.customer.name;
                 href resource_to_url(member.customer, request)" />
      </customer>
      <subprojects>
          <tal:block tal:repeat="sp member.subprojects">
            <subproject>
              <label tal:content="sp.label">Validation 01</label>
              <creationDate tal:content="rfc3339(sp.creation_date)">2010-11-08T11:16:37+01:00</creationDate>
              <active tal:content="str(sp.active).lower()">false</active>
            </subproject>
          </tal:block>
      </subprojects>
    </project>
  </content>
</feed>