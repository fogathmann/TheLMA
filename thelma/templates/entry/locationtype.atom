<?xml version="1.0" encoding="utf-8"?>
<entry xmlns="http://www.w3.org/2005/Atom"
       xmlns:tal="http://xml.zope.org/namespaces/tal"
       xmlns:metal="http://xml.zope.org/namespaces/metal"
       metal:use-macro="master.macros['standard_entry']">
  <content type="application/vnd.everest+xml;type=locationTypeMember"
           metal:fill-slot="content">
    <locationType xmlns="http://schemata.cenix-bioscience.com/location-type">
      <name tal:content="context.name">carrier slot</name>
      <tal:block tal:repeat="location context.locations">
      <link xmlns="http://www.w3.org/2005/Atom"
            rel="http://relations.cenix-bioscience.com/location"
            type="application/atom+xml;type=entry"
            title="c4s2" href="/locations/c4c2"
            tal:attributes="title location.title;
                            href resource_to_url(location, request)" />
      </tal:block>
    </locationType>
  </content>
</entry>