<?xml version="1.0" encoding="utf-8"?>
<entry xmlns="http://www.w3.org/2005/Atom"
       xmlns:tal="http://xml.zope.org/namespaces/tal"
       xmlns:metal="http://xml.zope.org/namespaces/metal"
       metal:use-macro="master.macros['standard_entry']">
  <content type="application/vnd.everest+xml;type=rackShapeMember"
           metal:fill-slot="content">
    <rackShape xmlns="http://schemata.cenix-bioscience.com/rack-shape">
      <label tal:content="context.label">8x12</label>
      <rows tal:content="context.number_rows">8</rows>
      <columns tal:content="context.number_columns">12</columns>
    </rackShape>
  </content>
</entry>