<?xml version="1.0" encoding="utf-8"?>
<entry xmlns="http://www.w3.org/2005/Atom"
       xmlns:tal="http://xml.zope.org/namespaces/tal"
       xmlns:metal="http://xml.zope.org/namespaces/metal"
       metal:use-macro="master.macros['standard_entry']">
  <content type="application/vnd.everest+xml;type=tubeSpecsMember"
           metal:fill-slot="content">
    <tubeSpecs xmlns="http://schemata.cenix-bioscience.com/container-specs">
      <label tal:content="context.label">Abgene tube (1.4 ml)</label>
      <description tal:content="context.description">Descriptive text about the tube</description>
      <maxVolume unit="l" tal:content="context.max_volume">0.0012</maxVolume>
      <deadVolume unit="l" tal:content="context.dead_volume">5e-06</deadVolume>
      <link xmlns="http://www.w3.org/2005/Atom"
            rel="http://relations.cenix-bioscience.com/organization"
            type="application/atom+xml;type=entry"
            title="AbGene" href="/organizations/abgene"
            tal:omit-tag="context.manufacturer is None"
            tal:attributes="title context.manufacturer.name;
                            href resource_to_url(context.manufacturer, request)" />
      <tal:block tal:repeat="specs context.rack_specs">
      <link xmlns="http://www.w3.org/2005/Atom"
            rel="http://relations.cenix-bioscience.com/rack-specs"
            type="application/atom+xml;type=entry"
            title="Abgene 96 tube rack" href="/rack-specs/abgene1400"
            tal:attributes="title specs.label;
                            href resource_to_url(specs, request)" />
      </tal:block>
    </tubeSpecs>
  </content>
</entry>