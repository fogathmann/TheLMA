<?xml version="1.0" encoding="utf-8"?>
<entry xmlns="http://www.w3.org/2005/Atom"
       xmlns:tal="http://xml.zope.org/namespaces/tal"
       xmlns:metal="http://xml.zope.org/namespaces/metal"
       metal:use-macro="master.macros['standard_entry']">
  <content type="application/vnd.everest+xml;type=tubeRackSpecsMember"
           metal:fill-slot="content">
    <tubeRackSpecs xmlns="http://schemata.cenix-bioscience.com/rack-specs">
      <label tal:content="context.label">Abgene 96 tube rack (1.4 ml)</label>
      <link xmlns="http://www.w3.org/2005/Atom"
            rel="http://relations.cenix-bioscience.com/rack-shape"
            type="application/atom+xml;type=entry"
            title="Nunc" href="/rack-shapes/nunc"
            tal:attributes="title context.shape.label;
                            href resource_to_url(context.shape, request)" />
      <hasTubes tal:content="context.has_tubes">false</hasTubes>
      <link xmlns="http://www.w3.org/2005/Atom"
            rel="http://relations.cenix-bioscience.com/organization"
            type="application/atom+xml;type=entry"
            title="Nunc" href="/organizations/nunc"
            tal:omit-tag="context.manufacturer is None"
            tal:attributes="title context.manufacturer.name;
                            href resource_to_url(context.manufacturer, request)" />
      <tal:block tal:repeat="specs context.tube_specs">
      <link xmlns="http://www.w3.org/2005/Atom"
            rel="http://relations.cenix-bioscience.com/container-specs"
            type="application/atom+xml;type=entry"
            title="Abgene tube (1.4 ml)" href="/container-specs/abgene1400"
            tal:attributes="title specs.label;
                            href resource_to_url(context, request)" />
      </tal:block>
    </tubeRackSpecs>
  </content>
</entry>