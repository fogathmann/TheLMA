<?xml version="1.0" encoding="UTF-8"?>
<schema
    targetNamespace="http://schemata.cenix-bioscience.com/experimentmetadata"
    elementFormDefault="qualified"
    xmlns="http://www.w3.org/2001/XMLSchema"
    xmlns:em="http://schemata.cenix-bioscience.com/experimentmetadata"
    xmlns:emt="http://schemata.cenix-bioscience.com/experimentmetadatatype"
    xmlns:ed="http://schemata.cenix-bioscience.com/experimentdesign"
    xmlns:isor="http://schemata.cenix-bioscience.com/isorequest"
    xmlns:mdps="http://schemata.cenix-bioscience.com/moleculedesignpoolset"
    xmlns:sh="http://schemata.cenix-bioscience.com/shared"
    xmlns:sp="http://schemata.cenix-bioscience.com/subproject"
>
    <import
        schemaLocation="Shared.xsd"
        namespace="http://schemata.cenix-bioscience.com/shared"
    >
    </import>

    <import
        schemaLocation="ExperimentDesign.xsd"
        namespace="http://schemata.cenix-bioscience.com/experimentdesign"
    >
    </import>

    <import
        schemaLocation="ExperimentMetadataType.xsd"
        namespace="http://schemata.cenix-bioscience.com/experimentmetadatatype"
    >
    </import>

    <import
        schemaLocation="MoleculeDesignPoolSet.xsd"
        namespace="http://schemata.cenix-bioscience.com/moleculedesignpoolset"
    >
    </import>

    <import
        schemaLocation="IsoRequest.xsd"
        namespace="http://schemata.cenix-bioscience.com/isorequest"
    >
    </import>

    <import
        schemaLocation="Subproject.xsd"
        namespace="http://schemata.cenix-bioscience.com/subproject"
    >
    </import>

    <complexType
        name="ExperimentMetadataType"
    >
        <complexContent>
            <extension
                base="sh:ResourceType"
            >
                <choice>
                    <element
                        name="link"
                        type="em:ExperimentMetadataLinkType"
                    >
                    </element>
                    <sequence>
                        <element
                            name="label"
                            type="string"
                            maxOccurs="1"
                        >
                        </element>
                        <element
                            name="ticket_number"
                            type="integer"
                            minOccurs="0"
                            maxOccurs="1"
                        >
                        </element>
                        <element
                            ref="sp:subproject"
                        >
                        </element>
                        <element
                        	ref="emt:experiment_metadata_type"
                        >
                        </element>
                        <element
                            name="number_replicates"
                            type="nonNegativeInteger"
                            minOccurs="0"
                            maxOccurs="1"
                        >
                        </element>
                        <element
                            ref="mdps:molecule_design_pool_set"
                            minOccurs="0"
                            maxOccurs="1"
                        >
                        </element>
                        <element
                            ref="ed:experiment_design"
                            minOccurs="0"
                            maxOccurs="1"
                        >
                        </element>
                        <element
                            ref="isor:iso_request"
                            minOccurs="0"
                            maxOccurs="1"
                        >
                        </element>
                        <element
                            name="creation_date"
                            type="dateTime"
                            minOccurs="0"
                            maxOccurs="1"
                        >
                        </element>
                    </sequence>
                </choice>
            </extension>
        </complexContent>
    </complexType>

    <complexType
        name="ExperimentMetadataCollectionType"
    >
        <sequence
            maxOccurs="unbounded"
            minOccurs="0"
        >
            <choice>
                <element
                    name="link"
                    type="em:ExperimentMetadataCollectionLinkType"
                    maxOccurs="1"
                    minOccurs="0"
                ></element>
                <element
                    ref="em:experiment_metadata"
                    maxOccurs="unbounded"
                    minOccurs="1"
                ></element>
            </choice>
        </sequence>
    </complexType>

    <complexType
        name="ExperimentMetadataLinkType"
    >
        <complexContent>
            <restriction
                base="sh:LinkType"
            >
                <attribute
                    name="rel"
                    type="string"
                    fixed="http://relations.cenix-bioscience.com/experiment-metadata"
                ></attribute>
            </restriction>
        </complexContent>
    </complexType>

    <complexType
        name="ExperimentMetadataCollectionLinkType"
    >
        <complexContent>
            <restriction
                base="sh:LinkType"
            >
                <attribute
                    name="rel"
                    type="string"
                    fixed="http://relations.cenix-bioscience.com/experiment-metadata-collection"
                ></attribute>
            </restriction>
        </complexContent>
    </complexType>

    <element
        name="experiment_metadata"
        type="em:ExperimentMetadataType"
    >
    </element>

    <element
        name="experiment_metadatas"
        type="em:ExperimentMetadataCollectionType"
    ></element>

</schema>