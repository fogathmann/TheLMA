>

    <!-- SHARED TYPES -->

    <xs:complexType
        name="ResourceType"
    >
        <xs:attribute
            name="id"
            use="optional"
            type="xs:string"
        >
        </xs:attribute>
    </xs:complexType>
    <xs:complexType
        name="LinkType"
    >
        <xs:attribute
            name="href"
            use="required"
            type="xs:anyURI"
        >
        </xs:attribute>
        <xs:attribute
            name="rel"
            type="xs:string"
        >
        </xs:attribute>
        <!-- <xs:attribute name="type" use="optional" type="xs:string" ></xs:attribute> -->
        <xs:attribute
            name="title"
            use="optional"
            type="xs:string"
        >
        </xs:attribute>
        <xs:attribute
            name="length"
            use="optional"
            type="xs:positiveInteger"
        >
        </xs:attribute>
    </xs:complexType>

    <xs:simpleType
        name="RackBarcodeType"
    >
        <xs:restriction
            base="xs:string"
        >
            <xs:pattern
                value="0\d{7}"
            ></xs:pattern>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType
        name="LocationBarcodeType"
    >
        <xs:restriction
            base="xs:string"
        >
            <xs:pattern
                value="0\d{7}"
            ></xs:pattern>
        </xs:restriction>
    </xs:simpleType>


